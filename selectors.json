{
  "abertay.ac.uk": {
    "course_name": {
      "primary": {
        "source": "course",
        "type": "single",
        "selector": "h1#headerBannerTitle"
      }
    },

    "duration_raw": {
      "primary": {
        "source": "course",
        "type": "by_label",
        "container_selector": "div.e-item",
        "label": "Duration"
      }
    },

    "available_intakes_raw": {
      "primary": {
        "source": "course",
        "type": "by_label",
        "container_selector": "div.e-item",
        "label": "Start Date"
      }
    },

    "international_tuition_fee_raw": {
      "primary": {
        "source": "course",
        "type": "table",
        "selector": "table:has(a:-soup-contains(\"International\"))"
      }
    },

    "english_test_requirement_raw": {
      "fallback": {
        "source": "external",
        "url": "https://www.abertay.ac.uk/international/english-language-requirements/",
        "type": "blocks",
        "selector": "ul > li.preamble > p.preamble"
      }
    },

    "indian_entry_requirement_raw": {
      "fallback": {
        "source": "external",
        "url": "https://www.abertay.ac.uk/international/south-asia/india/",
        "type": "blocks",
        "selector": "div.e-item:has(h3:-soup-contains(\"Undergraduate\")) div.more"
      }
    }
  }
}
